<h1 class="text-center">Home</h1>
<hr>
<div class="container text-center">
	<div class="row">
		{{#each products}}
			{{#unless this.Purchased}}
		<div class="col-md-3">
			<div class="well well-lg">
				
				{{this.productname}}
                <img class="images" src={{this.imageurl}}>
                <form action="/index?_method=PUT" method="POST">
                    <input class="form-control" type="hidden" name="productname">
                </form>

			</div>
		</div>
			{{/unless}}
		{{/each}}
	</div>
</div>